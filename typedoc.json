{
    "entryPoints": ["./src/*"], // 指定了 TypeScript 源文件的入口点
    "exclude": [
        "./src/User.ts",
        // "**/__tests__/**",
        // "**/node_modules/**"
      ],                    // 排除打包的文件目录
